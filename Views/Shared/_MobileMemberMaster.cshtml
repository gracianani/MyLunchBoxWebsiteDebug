<!DOCTYPE HTML>
<html manifest="" lang="en-US">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="@Url.Content("~/Content/jquery.mobile-1.3.0-beta.1.css")" type="text/css">
    <link rel="stylesheet" href="@Url.Content("~/Content/hicfood-theme.css")" type="text/css">
    
    
    <link rel="stylesheet" href="@Url.Content("~/Content/mobile.css")" type="text/css">
    <!-- The line below must be kept intact for Sencha Command to build your application -->
    <script src="@Url.Content("~/Scripts/modernizr-2.6.2-respond-1.1.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.8.3.min.js")" type="text/javascript"></script>
    <script>
	$(document).on("mobileinit", function(){
	  $.extend(  $.mobile , {
		ajaxEnabled: false
	  });
	});
	</script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script id="microloader" src="@Url.Content("~/Scripts/jquery.mobile-1.3.0-beta.1.min.js")" type="text/javascript"></script>

</head>
<body>
<div data-role="page" data-theme="a" class="hicpage">
  <div data-role="header" class="hicbg" >
    <a data-rel="back" data-icon="back" data-inline="true" data-mini="true" data-theme="b" data-corners="false" data-iconPos="notext" data-ajax="false">Back</a><h1>@ViewBag.Title</h1><a href="/Mobile/" data-icon="home" data-inline="true" data-mini="true" data-theme="b" data-corners="false" data-iconPos="notext" data-ajax="false">Home</a>
  </div>
  <!-- /header -->
        @RenderBody()
        

</div>
<!-- /page -->

</body>
</html>
